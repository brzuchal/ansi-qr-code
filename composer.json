{
    "name": "brzuchal/ansi-qr-code",
    "description": "PHP library for creating QR codes in ANSI format for terminal",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brzuchal/coding-standard"
        }
    ],
    "require": {
        "php": "^8.3",
        "bacon/bacon-qr-code": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "brzuchal/coding-standard": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brzuchal\\TerminalQr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brzuchal\\TerminalQr\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}